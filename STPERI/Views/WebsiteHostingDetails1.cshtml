@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.WebsiteHostingDetails1>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}
</header>

    <div class="innerbanner fitlayout">

        <h3 class="px-3 aos-item" data-aos="fade-up">@Model.TrainingTitle</h3>
        <div class="clearfix"></div>
        <ul class="listbenner px-3 aos-item" data-aos="fade-up">
          <li><b>@Model.Price</b></li>
          <li> @Model.Author</li>
          <li>@Model.Startdate</li>
          <li>@Model.Category</li>
          <li>@Model.Traininglevel</li>
        </ul>
         
       
        <img alt="" src="@Model.BannerImage.Url()" />
    </div>


    <div class="w-bg fitlayout pt-4 mb-4 paddingabove">

      <div class="container-fluid">
           
           <div class="row">

            <div class="col-lg-10 my-4 aos-item" data-aos="fade-dowb">
              <h2 class="migatitle mb-4">@Model.OfferTitle</h2>
            </div>


            <div class="col-lg-6 mb-4 aos-item" data-aos="fade-right">
              <div class="quoatline">
                 @Model.OfferFirstDescription          </div>
             </div>


             <div class="col-lg-6 mb-4 aos-item" data-aos="fade-left">
              <div class="quoatline">
              @Model.OfferSecondDescription      </div>
             </div>

             <div class="col-lg-6 mb-4 aos-item" data-aos="fade-right">
              <div class="quoatline">
               @Model.OfferThirdDescription            </div>
             </div>

             <div class="col-lg-6 mb-4 aos-item" data-aos="fade-left">
              <div class="quoatline">
              @Model.OfferForthDescription             </div>
             </div>

             <div class="col-lg-10 my-5 aos-item" data-aos="fade-up">
              <h2 class="migatitle mb-4">@Model.GalleryTitle</h2>
            </div>

             
           
             <div class="col-lg-6 mb-4 aos-item" data-aos="zoom-in">
              <div class="galleryitem">
                <a href="@Model.FirstImageGallery.Url()" class="fancybox" data-fancybox-group="gallery"></a>
                
                    <img alt="" class="w-100" src="@Model.FirstImageGallery.Url()" />
              
               </div>
             </div>

             <div class="col-lg-6 mb-4 aos-item" data-aos="zoom-in">
				<div class="galleryitem">
					<a href="@Model.SecondImageGallery.Url()" class="fancybox" data-fancybox-group="gallery"></a>
					
						<img alt="" class="w-100" src="@Model.SecondImageGallery.Url()" />
				
				</div>
             </div>

             <div class="col-lg-6 mb-4 aos-item" data-aos="zoom-in">
				<div class="galleryitem">
					<a href="@Model.ThirdImageGallery.Url()" class="fancybox" data-fancybox-group="gallery"></a>
					
						<img alt="" class="w-100" src="@Model.ThirdImageGallery.Url()" />
				
				</div>
             </div>

             <div class="col-lg-6 mb-4 aos-item" data-aos="zoom-in">
				<div class="galleryitem">
					<a href="@Model.ForthImageGallery.Url()" class="fancybox" data-fancybox-group="gallery"></a>
					
						<img alt="" class="w-100" src="@Model.ForthImageGallery.Url()" />
				
				</div>
             </div>


             <div class="col-lg-12 mb-4">
                 <div class="formholder">
                     <h3 class="px-3">Apply Now</h3>
                    <div class="col-lg-6">
 						@Model.Value("ApplyForm")
                    </div>
                 </div>
             </div>
             
           
           </div>
      </div>

    </div>