   @inherits UmbracoViewPage<BlockListItem>
    @{
      var row = (CentralLabsRow)Model.Content;
      var settings = (CentralLabsRowSettings)Model.Settings;
      if(settings?.Hide ?? false){return;}
      if(row.CentralLabsItems == null || !row.CentralLabsItems.Any()) return;   
    }

          
           <div class="row">

            <div class="col-lg-12 mt-4">
              <h2 class="migatitle mb-0">@row.Title</h2>
            </div>

            <div class="col-lg-12">

          
            <div class="position-relative px-0 py-4 labtabs" >
             
              <div class="carsulholder">
                <div class="owl-carouselsteps">
          @foreach(var item in row.CentralLabsItems)
          {
           var rowItem = (CentralLabsItem)item.Content;
                    <div class="item">
                      <div class="labsblock">
                        <div class="imglab">
                            <a href="/centrallabsdetails"></a>
                            <i class="feather icon-arrow-right"></i>
                            <img alt="Lab Name" src="@rowItem.Image.Url()" />
                        </div>
                        <h5>@rowItem.Title </h5>
                        <span>@rowItem.Description
                       </span>
                    </div>
                  </div>
          }
                </div>
            </div>
            
             
            </div>

             </div>
          
           </div>