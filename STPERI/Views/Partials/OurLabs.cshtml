   @inherits UmbracoViewPage
    @{
      var homePage = Model.AncestorOrSelf<HomePage>();
    }
    <div class="w-bg startfit py-4 aos-item" data-aos="fade-left">

      <div class="container-fluid">
           
           <div class="row">

            <div class="col-lg-12 mt-4">
              <h2 class="migatitle">@homePage.OurLabsTitle</h2>
            </div>

            <div class="col-lg-12">

            <ul class="nav nav-tabs labseri" id="myTab" role="tablist">
              <li class="nav-item" role="presentation">
                <button class="nav-link active" id="Central-tab" data-bs-toggle="tab" data-bs-target="#Central-tab-pane" type="button" role="tab" aria-controls="Central-tab-pane" aria-selected="true">@homePage.CentrallabsButton</button>
              </li>
              <li class="nav-item" role="presentation">
                <button class="nav-link" id="Research-tab" data-bs-toggle="tab" data-bs-target="#Research-tab-pane" type="button" role="tab" aria-controls="Research-tab-pane" aria-selected="false">@homePage.ResearchDepartmentsButton</button>
              </li>
              <li class="nav-item" role="presentation">
                <button class="nav-link" id="Specialized-tab" data-bs-toggle="tab" data-bs-target="#Specialized-tab-pane" type="button" role="tab" aria-controls="Specialized-tab-pane" aria-selected="false">@homePage.SpecializedLabsButton</button>
              </li>
             
            </ul>
            <div class="tab-content position-relative px-0 py-4 labtabs" id="myTabContent">
              <div class="tab-pane bg-white fade show active" id="Central-tab-pane" role="tabpanel" aria-labelledby="Central-tab" tabindex="0">
                <div class="carsulholder">
                  <div class="owl-carouselsteps">
                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                              <a href="/centrallabsdetails"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.CentralLabsFirstImage.Url()" />
                          </div>
                          <h5>@homePage.CentralLabsFirstTitle </h5>
                          <span>@homePage.CentralLabsFirstDescription
                         </span>
                     </div>
                      </div>

                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                              <a href="/centrallabsdetails"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.CentralLabsSecondImage.Url()" />
                          </div>
                          <h5>@homePage.CentralLabsSecondTitle</h5>
                          <span>@homePage.CentralLabsSecondDescription
                         </span>
                     </div>
                      </div>

                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                              <a href="/centrallabsdetails"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.CentralLabsThirdImage.Url()" />
                          </div>
                          <h5>@homePage.CentralLabsThirdTitle</h5>
                          <span>
                            @homePage.CentralLabsThirdDescription
                         </span>
                        </div>
                      </div>

                     
                  </div>
              </div>
              </div>
              <div class="tab-pane bg-white fade" id="Research-tab-pane" role="tabpanel" aria-labelledby="Research-tab" tabindex="0">
                <div class="carsulholder">
                  <div class="owl-carouselsteps">
                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                              <a href="/researchdepartments"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.ResearchDepartmentsFirstImage.Url()" />
                          </div>
                          <h5>@homePage.ResearchDepartmentsFirstTitle </h5>
                          <span>@homePage.ResearchDepartmentsFirstDescription
                         </span>
                     </div>
                      </div>

                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                              <a href="/researchdepartments"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.ResearchDepartmentsSecondImage.Url()" />
                          </div>
                          <h5>@homePage.ResearchDepartmentsSecondTitle</h5>
                          <span>@homePage.ResearchDepartmentsSecondDescription
                         </span>
                     </div>
                      </div>

                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                              <a href="/researchdepartments"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.ResearchDepartmentsThirdImage.Url()" />
                          </div>
                          <h5>@homePage.ResearchDepartmentsThirdTitle</h5>
                          <span>
                              @homePage.ResearchDepartmentsThirdDescription 
                          </span>
                        </div>
                      </div>

                     
                  </div>
              </div>
              </div>
              <div class="tab-pane bg-white fade" id="Specialized-tab-pane" role="tabpanel" aria-labelledby="Specialized-tab" tabindex="0">
                <div class="carsulholder">
                  <div class="owl-carouselsteps">
                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                                            <a href="/specializedlabs"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.SpecializedLabsFirstImage.Url()" />
                          </div>
                          <h5>@homePage.SpecializedLabsFirstTitle </h5>
                          <span>@homePage.SpecializedLabsFirstDescription
                         </span>
                     </div>
                      </div>

                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                                            <a href="/specializedlabs"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.SpecializedLabsSecondImage.Url()" />
                          </div>
                          <h5>@homePage.SpecializedLabsSecondTitle</h5>
                          <span>@homePage.SpecializedLabsSecondDescription
                         </span>
                     </div>
                      </div>

                      <div class="item">
                        <div class="labsblock">
                          <div class="imglab">
                                            <a href="/specializedlabs"></a>
                              <i class="feather icon-arrow-right"></i>
                              <img alt="Lab Name" src="@homePage.SpecializedLabsThirdImage.Url()" />
                          </div>
                          <h5>@homePage.SpecializedLabsThirdTitle</h5>
                          <span>
                           @homePage.SpecializedLabsThirdDescription
                         </span>
                        </div>
                      </div>

                     
                  </div>
              </div>
              </div>
             
            </div>

             </div>
           

          </div>
        </div>
    </div>